{
  "name": "@enymo/glissade",
  "version": "0.1.6",
  "description": "Utility hooks for input creation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      }
    }
  },
  "tsup": {
    "entry": [
      "src/js/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enymo/glissade.git"
  },
  "author": "enymo GmbH",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.17.4",
    "@types/react": "^18.3.3",
    "postcss": "^8.4.17",
    "tsup": "^8.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@enymo/react-form-component": "^1.6.2",
    "@enymo/react-hybrid-input-hook": "^1.1.4",
    "react": ">=17.0.0",
    "react-hook-form": "^7.51.5"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "lodash": "^4.17.21"
  }
}
